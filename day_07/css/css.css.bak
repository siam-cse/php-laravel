.row {
    float: left;
  }
.row {
    contain: '';
    clear: both;
    display: block;
  }
.box-one{
    height: 300px;
    width: 500px;
    background-color: #006747;
    position: relative;
}.box-two{
    height: 200px;
    width: 200px;
    background-color: #DA291C;
    position:absolute;
    top: 50%;
    left: 50%; 
    border-radius: 50%;
     transform: translate(-50%, -50%);
}
.box-three{
    height: 200px;
    width: 200px;
    float: left;
    background-color:greenyellow;   
    transition: 2s;
    margin: 150px;
}
.box-three:hover{
    background-color:greenyellow;
    transform: scale(1.5,3);
} 
 

.box-four{
    height: 200px; 
    width: 200px;
    float: left;
    margin-left: 100px;
    margin-top: 50px;
    margin-bottom: 50px;
    background-color:green; 
    transition: 2s;
}.box-four:hover{
    background-color:red; 
    transform: rotateX(360deg);
    transform: rotateY(360deg);
    transform: rotate(360deg);
    border-radius: 50%;
}
.box-five{
    height: 200px; 
    width: 200px;
    float: left;
    background-color:chocolate; 
    margin: 50px;
    transition: 1s;
}.box-five:hover{
    transform: translate(200px);
    transform: translateY(200px);
    transform: translateY(-200px);
    transform: translate(200px,-150px);
}.box-six{
    height: 300px; 
    width: 200px;
    float: left;
    background-color:rgb(159, 30, 210); 
    margin: 50px;
    transition: 3s;
}.box-six:hover{
    transform: skewX(360deg) ;
}

.div1{
    margin-left: 100px;
    margin-bottom: 50px;
    height: 200px; 
    width: 200px;
    background-color:green; 
    float: left;
    transform: skewX(45deg) ;
}.div2{
    margin-left: 10px;
    height: 200px; 
    width: 200px;
    background-color:chocolate; 
    float: left;
    transform: skewX(-45deg) ;
}
.fan1, .fan2,.fan3{
    float: left;
    height: 200px;
    width: 200px;
    background-color: orangered;
    transform: rotate(45deg);
    margin-left: 150px;
    margin-top: 10px;
    margin-bottom: 50px;
    animation: seip 3s infinite;
    /*animation-name: seip;*/
    /*animation-duration: 4s;*/
    /*animation-iteration-count: infinite;*/
}

@keyframes seip {
    0% { transform: rotate(0deg); }
    100% { transform: rotate(3600deg); }
}
 